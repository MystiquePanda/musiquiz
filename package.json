{
  "name": "musiquiz",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "eslint": "eslint \"src/**/*.js\"",
    "start": "concurrently  \"yarn run dev:bundle\" \"yarn run dev:start\" \"yarn pm2 logs\"",
    "dev:server": "cross-env NODE_PATH=./src nodemon --exec \"babel-node src/server/server.js\" --ignore .reactful.json --ignore public/",
    "dev:start": "cross-env NODE_PATH=./src pm2 start src/server/server.js --watch --interpreter babel-node",
    "dev:bundle": "webpack -wd",
    "build:react": "cross-env NODE_ENV=production webpack --progress -p",
    "build:node": "babel src -d build --config-file ./babel-node.config.js --copy-files",
    "build:all": "yarn install && yarn run build:react && yarn run build:node",
    "prod:start": "cross-env NODE_ENV=production NODE_PATH=./build pm2 start -i max build/server/server.js --update-env --name .Prod",
    "prod:stop": "pm2 stop .Prod",
    "prod:reload": "pm2 reload --update-env .Prod",
    "prod:logs": "pm2 logs --update-env .Prod"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.2",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "client-sessions": "^0.8.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "ejs": "^3.1.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "isomorphic-fetch": "^2.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "mongodb": "^3.5.9",
    "morgan": "^1.10.0",
    "node-uuid": "^1.4.8",
    "pm2": "^4.4.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "regenerator-runtime": "^0.13.5",
    "serialize-javascript": "^3.0.0",
    "style-loader": "^1.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/node": "^7.8.7",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@testing-library/user-event": "^10.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "reactful": "^2.2.0"
  }
}
